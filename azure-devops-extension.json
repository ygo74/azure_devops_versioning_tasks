        {
            "path": "_build/Tasks/CalculateNextVersionV1"
        }
    ],
    "contributions": [
        {
            "id": "ygo74-Versioning-tasks",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "CalculateNextVersion"
            }
        }
    ]
}