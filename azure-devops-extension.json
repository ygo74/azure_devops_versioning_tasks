{
    "manifestVersion": 1,
    "id": "Versioning-tasks",
    "publisher": "ygo74",
    "version": "1.0.6",
    "name": "Versioning-tasks",
    "description": "Tasks to calculate next version and to update application versions",
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/success.png"
    },
